<% unless admin_signed_in? %>
<% unless current_user.name == "guestuser" %>
  <% if image.favorited_by?(current_user) %>
      <%= link_to post_image_favorites_path(image), method: :delete, remote: true do %>
        ♥<%= image.favorites.count %>
      <% end %>
  <% else %>
      <%= link_to post_image_favorites_path(image), method: :post, remote: true do %>
       ♡<%= image.favorites.count %>
      <% end %>
    </p>
  <% end %>
<% end %>
<% end %>