<div class='container mt-5'>
   <div class='row'>
     <h4>edit category</h4>
   </div>
   <%= form_with model: @q_category,local:true, url: edit_admin_question_category_path, method: :patch do |f| %>
     <div class="form-group row">
        <div class="col-2">
          <%= f.label "カテゴリー名" %>
        </div>
        <div class="col-3">
          <%= f.text_field :q_category, class: 'form-control q_category' %>
        </div>
        <div class="col-2">
          <%= f.submit "変更を保存",class: "btn btn-outline-success" %>
        </div>
      </div>
    <% end %>
  </div>